import java.io.*;
import java.util.Scanner;
public class Main {
public static void main(String args[])throws IOException
{
	Scanner s1=new Scanner(System.in);
	int T=Integer.parseInt(s1.next());
	int counter=0;
	for(int i=0;i<T;i++)
	{
		int p=Integer.parseInt(s1.next());
		System.out.println((int)(Math.ceil(Math.log(p+1)/Math.log(2))));
	}
}
}
