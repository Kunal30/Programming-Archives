import java.util.*;
public class Main {
	public static void main(String[] args)
	{
		BST bst = new BST();
        bst.insert(5);
        bst.insert(4);
        bst.insert(3);
        bst.insert(2);
        bst.insert(6);
        bst.insert(7);
        System.out.println("Average depth="+bst.computeAverageDepthOfTree());
        System.out.println("New Average depth after rotation="+bst.computeAverageDepthOfTree());
	}
}
