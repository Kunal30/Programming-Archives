.ORIG X10
MOV B X5,R2
MOV B X4,R3
MOV B X3,R4
MOV B X2,R5
MOV B X1,R6
STORE B [X200],R2	
STORE B [X201],R3
STORE B [X202],R4
STORE B [X203],R5
STORE B [X204],R6
MOV B X5,R0
MOV W [X204],R8
SORT: MOV B R0,R1
MOV W X200,R7
LOAD B [R7],R10
MOV W R7,R12

LOOP: ADD W X1,R7
LOAD B [R7],R11
SUB B X1,R1
COMP B R10,R11
JN LOOPRUN
MOV B R11,R10
MOV W R7,R12
LOOPRUN: COMP B X1,R1
JNZ LOOP
LOAD B [R8],R9
STORE B [R8],R10
STORE B [R12],R9
SUB B X1,R8
SUB B X1,R0
COMP B X1,R0
JNZ SORT
HALT
.END